<time-delay>
  <simulation>
    <params>
      <trajectory>
        <A>20.0</A>
        <f>0.05</f>
      </trajectory>
      <T>5000.0</T>
      <dt>0.1</dt>
      <calibration>
        <odometry>
          <wss>
            <b>0.4</b>
            <k_l>360.0</k_l>
            <t_l>0.0</t_l>
            <k_r>360.0</k_r>
            <t_r>0.0</t_r>
          </wss>
        </odometry>
        <v_T_p>
          <v_r_vp>
            <e1>0.0</e1>
            <e2>0.0</e2>
            <e3>0.0</e3>
          </v_r_vp>
          <v_R_p>
            <e1>0.0</e1>
            <e2>0.0</e2>
            <e3>0.0</e3>
          </v_R_p>
        </v_T_p>
      </calibration>
      <noise>
        <odometry>
          <wss>
            <sigma2_l>1e-3</sigma2_l>
            <sigma2_r>1e-3</sigma2_r>
          </wss>
        </odometry>
        <pose>
          <sigma2_w_r_wp>
            <e1>1e-6</e1>
            <e2>1e-6</e2>
            <e3>1e-6</e3>
          </sigma2_w_r_wp>
          <sigma2_w_R_p>
            <e1>1e-6</e1>
            <e2>1e-6</e2>
            <e3>1e-6</e3>
          </sigma2_w_R_p>
        </pose>
      </noise>
    </params>
  </simulation>

  <calibrator>
    <windowDuration>1e18</windowDuration>
    <verbose>true</verbose>
    <useTimeDelay>true</useTimeDelay>
    <splines>
      <transSplineLambda>1e-3</transSplineLambda>
      <rotSplineLambda>1e-3</rotSplineLambda>
      <splineKnotsPerSecond>5</splineKnotsPerSecond>
      <transSplineOrder>4</transSplineOrder>
      <rotSplineOrder>4</rotSplineOrder>
    </splines>
    <odometry>
      <sensors>
        <wss>
          <noise>
            <lwVariance>130</lwVariance>
            <rwVariance>130</rwVariance>
          </noise>
        </wss>
      </sensors>
      <intrinsics>
        <halfWheelBase>0.4</halfWheelBase>
        <lwCoefficient>360.0</lwCoefficient>
        <rwCoefficient>360.0</rwCoefficient>
        <lwTimeDelay>0.0</lwTimeDelay>
        <rwTimeDelay>0.0</rwTimeDelay>
        <timeDelaysActive>true</timeDelaysActive>
      </intrinsics>
      <extrinsics>
        <translation>
          <active>false</active>
          <x>0.0</x>
          <y>0.0</y>
          <z>0.0</z>
        </translation>
        <rotation>
          <active>false</active>
          <yaw>0.0</yaw>
          <pitch>0.0</pitch>
          <roll>0.0</roll>
        </rotation>
      </extrinsics>
      <constraints>
          <noise>
            <vyVariance>1e-3</vyVariance>
            <vzVariance>1e-3</vzVariance>
          </noise>
      </constraints>
    </odometry>
    <estimator>
      <infoGainDelta>0.5</infoGainDelta>
      <groupId>1</groupId>
      <verbose>true</verbose>
      <optimizer>
        <convergenceDeltaJ>1e-3</convergenceDeltaJ>
        <convergenceDeltaX>1e-3</convergenceDeltaX>
        <maxIterations>20</maxIterations>
        <linearSolverMaximumFails>0</linearSolverMaximumFails>
        <nThreads>16</nThreads>
        <verbose>true</verbose>
        <linearSolver>
          <columnScaling>true</columnScaling>
          <epsNorm>1e-16</epsNorm>
          <epsSVD>1e-3</epsSVD>
          <epsQR>1e-16</epsQR>
          <svdTol>-1</svdTol>
          <qrTol>-1</qrTol>
          <verbose>true</verbose>
        </linearSolver>
      </optimizer>
    </estimator>
  </calibrator>
</time-delay>
