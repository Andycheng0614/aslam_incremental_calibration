<car>
  <calibrator>
    <windowDuration>1e18</windowDuration>
    <verbose>true</verbose>
    <splines>
      <transSplineLambda>0</transSplineLambda>
      <rotSplineLambda>0</rotSplineLambda>
      <splineKnotsPerSecond>5</splineKnotsPerSecond>
      <transSplineOrder>4</transSplineOrder>
      <rotSplineOrder>4</rotSplineOrder>
    </splines>
    <odometry>
      <sensors>
        <wheelSpeedSensorCutoff>350</wheelSpeedSensorCutoff>
        <linearVelocityTolerance>1</linearVelocityTolerance>
        <fws>
          <active>true</active>
          <noise>
            <flwPercentError>0.2</flwPercentError>
            <frwPercentError>0.2</frwPercentError>
            <flwVariance>1000</flwVariance>
            <frwVariance>1000</frwVariance>
          </noise>
        </fws>
        <rws>
          <active>true</active>
          <noise>
            <rlwPercentError>0.2</rlwPercentError>
            <rrwPercentError>0.2</rrwPercentError>
            <rlwVariance>1000</rlwVariance>
            <rrwVariance>1000</rrwVariance>
          </noise>
        </rws>
        <st>
          <active>true</active>
          <noise>
            <steeringVariance>1</steeringVariance>
          </noise>
        </st>
        <dmi>
          <active>true</active>
          <noise>
            <dmiPercentError>0.1</dmiPercentError>
            <dmiVariance>1</dmiVariance>
          </noise>
        </dmi>
      </sensors>
      <intrinsics>
        <wheelBase>2.7</wheelBase>
        <halfRearTrack>0.74</halfRearTrack>
        <halfFrontTrack>0.755</halfFrontTrack>
        <steeringCoefficient0>0</steeringCoefficient0>
        <steeringCoefficient1> 0.001745329251994</steeringCoefficient1>
        <steeringCoefficient2>0</steeringCoefficient2>
        <steeringCoefficient3>0</steeringCoefficient3>
        <rlwCoefficient>0.002777777777778</rlwCoefficient>
        <rrwCoefficient>0.002777777777778</rrwCoefficient>
        <flwCoefficient>0.002777777777778</flwCoefficient>
        <frwCoefficient>0.002777777777778</frwCoefficient>
        <dmiCoefficient>1.0</dmiCoefficient>
      </intrinsics>
      <extrinsics>
        <translation>
          <x>0</x>
          <y>0</y>
          <z>0.785</z>
        </translation>
        <rotation>
          <yaw>0</yaw>
          <pitch>0</pitch>
          <roll>3.141592653589793</roll>
        </rotation>
      </extrinsics>
      <constraints>
        <active>false</active>
          <noise>
            <vyVariance>1e-1</vyVariance>
            <vzVariance>1e-1</vzVariance>
            <omxVariance>1e-1</omxVariance>
            <omyVariance>1e-1</omyVariance>
          </noise>
      </constraints>
    </odometry>
    <estimator>
      <groupId>1</groupId>
      <miTol>0.5</miTol>
      <verbose>true</verbose>
  <!--    commenting tags leave them to their default value-->
      <optimizer>
        <convergenceDeltaJ>1e-3</convergenceDeltaJ>
        <convergenceDeltaX>1e-3</convergenceDeltaX>
        <maxIterations>20</maxIterations>
        <linearSolverMaximumFails>0</linearSolverMaximumFails>
        <nThreads>16</nThreads>
        <verbose>true</verbose>
        <linearSolver>
          <columnScaling>false</columnScaling>
<!--          <epsNorm>1e-16</epsNorm>-->
<!--          <epsSVD>1e-3</epsSVD>-->
<!--          <epsQR>1e-16</epsQR>-->
<!--          <svdTol>-1</svdTol>-->
<!--          <qrTol>-1</qrTol>-->
          <verbose>true</verbose>
        </linearSolver>
      </optimizer>
    </estimator>
  </calibrator>
  <simulation>
    <x0>
      <x>0</x>
      <y>0</y>
      <z>0</z>
      <yaw>0.392699081698724</yaw>
      <pitch>0</pitch>
      <roll>0</roll>
    </x0>
    <timestep>10000000</timestep>
    <numSteps>10000</numSteps>
    <splineKnotsPerSecond>5</splineKnotsPerSecond>
    <splineLambda>1e+4</splineLambda>
    <applanixFrequency>100</applanixFrequency>
    <wheelSpeedFrequency>60</wheelSpeedFrequency>
    <calibration>
      <odometry>
        <intrinsics>
          <wheelBase>2.7</wheelBase>
          <halfRearTrack>0.74</halfRearTrack>
          <halfFrontTrack>0.755</halfFrontTrack>
          <steeringCoefficient0>0</steeringCoefficient0>
          <steeringCoefficient1> 0.001745329251994</steeringCoefficient1>
          <steeringCoefficient2>0</steeringCoefficient2>
          <steeringCoefficient3>0</steeringCoefficient3>
          <rlwCoefficient>0.002777777777778</rlwCoefficient>
          <rrwCoefficient>0.002777777777778</rrwCoefficient>
          <flwCoefficient>0.002777777777778</flwCoefficient>
          <frwCoefficient>0.002777777777778</frwCoefficient>
          <dmiCoefficient>1.0</dmiCoefficient>
        </intrinsics>
        <extrinsics>
          <translation>
            <x>0</x>
            <y>0</y>
            <z>0.785</z>
          </translation>
          <rotation>
            <yaw>0</yaw>
            <pitch>0</pitch>
            <roll>3.141592653589793</roll>
          </rotation>
        </extrinsics>
      </odometry>
    </calibration>
  </simulation>
</car>
