<camera>
<!--  <rosTopic>/mv_37/camera/image_raw</rosTopic>-->
  <rosTopic>/mv_cameras_manager/GX002537/image_raw</rosTopic>
  <cameraId>GX002537</cameraId>
  <imageTime>1</imageTime>
  <calibrator>
    <saveEstimatorImages>true</saveEstimatorImages>
    <outputErrors>true</outputErrors>
    <rows>6</cols>
    <cols>7</cols>
    <rowSpacingMeters>0.06</rowSpacingMeters>
    <colSpacingMeters>0.06</colSpacingMeters>
    <useAdaptiveThreshold>true</useAdaptiveThreshold>
    <normalizeImage>true</normalizeImage>
    <filterQuads>false</filterQuads>
    <doSubpixelRefinement>true</doSubpixelRefinement>
    <showExtractionVideo>false</showExtractionVideo>
    <plotCornerReprojection>false</plotCornerReprojection>
    <imageStepping>false</imageStepping>
    <filterCornerOutliers>true</filterCornerOutliers>
    <filterCornerSigmaThreshold>2.0</filterCornerSigmaThreshold>
    <filterCornerMinReprojError>0.2</filterCornerMinReprojError>
    <cameraProjectionType>omni</cameraProjectionType>
    <estimateLandmarks>false</estimateLandmarks>
    <landmarksGroupId>2</landmarksGroupId>
    <calibrationGroupId>0</calibrationGroupId>
    <transformationsGroupId>1</transformationsGroupId>
    <batchNumImages>1</batchNumImages>
    <useMEstimator>false</useMEstimator>
    <sigma2>1.0</sigma2>
    <verbose>true</verbose>
    <estimator>
      <checkValidity>true</checkValidity>
      <infoGainDelta>0.2</infoGainDelta>
      <groupId>0</groupId>
      <verbose>true</verbose>
      <optimizer>
        <convergenceDeltaJ>1e-3</convergenceDeltaJ>
        <convergenceDeltaX>1e-3</convergenceDeltaX>
        <maxIterations>20</maxIterations>
        <linearSolverMaximumFails>0</linearSolverMaximumFails>
        <nThreads>16</nThreads>
        <verbose>true</verbose>
        <linearSolver>
          <columnScaling>true</columnScaling>
          <epsNorm>1e-16</epsNorm>
          <epsSVD>1e-6</epsSVD>
          <epsQR>1e-16</epsQR>
          <svdTol>-1</svdTol>
          <qrTol>-1</qrTol>
          <verbose>true</verbose>
        </linearSolver>
      </optimizer>
    </estimator>
  </calibrator>
  <validator>
    <visualization>true</visualization>
    <saveImages>false</saveImages>
    <outputErrors>true</outputErrors>
  </validator>
</camera>
